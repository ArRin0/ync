div>\n    <div class=\"kanBan-content-box\">\n      <div class=\"text-r margin-t10 margin-r20\">\n        <el-cascader\n          v-model=\"cityVal\"\n          :props=\"props\"\n          :options=\"cityJson\"\n          @change=\"handleChange\"\n        ></el-cascader>\n      </div>\n      <div id=\"bar\" :style=\"{width:width,height:'85%'}\" />\n    </div>\n  </div>\r\n  \n</template>\n\n<script>\nimport echarts from \"echarts\";\nimport { mapGetters } from \"vuex\";\nimport cityJson from \"@/assets/city1.json\";\nrequire(\"echarts/lib/chart/bar\");\nexport default {\n  //  柱形图\n  name: \"bar\",\n  data() {\n    return {\n      chart: null,\n      cityVal: [\"6100\", \"6101\"],\n      props: {\n        label: \"name\",\n        value: \"code\",\n        children: \"cities\"\n      },\n      cityJson: cityJson\n    };\n  },\n  computed: {\n    ...mapGetters([\"screenWidth\"])\n  },\n  props: {\n    className: {\n      type: String,\n      default: \"chart\"\n    },\n    width: {\n      type: String,\n      default: \"100%\"\n    },\n    height: {\n      type: String,\n      default: \"200px\"\n    },\n    getData: {\n      type: Array,\n      default: []\n    },\n    title: {\n      type: String\n    }\n  },\n  mounted() {\n    let _this = this;\n    this.$nextTick(() => {\n      setTimeout(() => {\n        _this.initChart();\n      }, 2000);\n    });\n  },\n  beforeDestroy() {\n    if (!this.chart) {\n      return;\n    }\n    this.chart.clear();\n    this.chart = null;\n  },\n  methods: {\n    handleChange(data) {\n      console.log(data);\n    },\n    getX(data) {\n      let x = [];\n      data.forEach(el => {\n        x.push(el.name);\n      });\n      return x;\n    },\n    getY(data) {\n      let y = [];\n      data.forEach(el => {\n        y.push(el.value);\n      });\n      return y;\n    },\n\n    initChart() {\n      let _this = this;\n      this.chart = echarts.init(document.getElementById(\"bar\"));\n      // console.log(_this.title)\n      this.chart.setOption({\n        title: {\n          show: false\n        },\n\n        color: [\"#1FE2FF\"],\n        tooltip: {\n          trigger: \"axis\",\n          axisPointer: {\n            // 坐标轴指示器，坐标轴触发有效\n            type: \"line\" // 默认为直线，可选为：'line' | 'shadow'\n          }\n        },\n        textStyle: {\n          fontSize: 15,\n          color: \"#56C0FE\"\n        },\n        grid: {\n          top:'3%',\n          left: \"3%\",\n          right: \"4%\",\n          bottom: \"10%\",\n          containLabel: true\n        },\n        xAxis: [\n          {\n            type: \"category\",\n            data: _this.getX(_this.getData),\n            axisTick: {\n              alignWithLabel: true\n            },\n            axisLabel: {\n              formatter: function(value) {\n                return value.split(\"\").join(\"\\n\");\n              }\n            }\n          }\n        ],\n        yAxis: [\n          {\n            type: \"value\",\n            splitLine: {\n              lineStyle: {\n                // 使用深浅的间隔色\n                color: [\"#0b234e\"]\n              }\n            }\n          }\n        ],\n        series: [\n          {\n            name: \"数量\",\n            type: \"bar\",\n            barWidth: \"30%\",\n            data: _this.getY(_this.getData),\n            // 修改圆角\n            itemStyle: {\n              normal: {\n                barBorderRadius: [0, 0, 0, 0]\n              }\n            }\n          }\n        ]\n      });\n    }\n  },\n  watch: {\n    screenWidth: function(el) {\n      let _this = this;\n      if (this.chart) {\n        this.chart.dispose();\n      }\n      setTimeout(() => {\n        _this.initChart();\n      }, 2000);\n    }\n  }\n};\n</script>\r\n\n<style scoped >\n.kanBan-box >>> .el-cascader .el-input .el-input__inner {\n  background-color: #050a2d;\n  height: 30px;\n  border: none;\n  width: 150px;\n  color: #ffffff;\n}\n.el-cascader__dropdown {\n  background-color: #050a2d !important;\n  border: none;\n  color: #ffffff;\n}\n.margin-r20{\n  margin-right: 20px;\n}\n</style>"]}]}